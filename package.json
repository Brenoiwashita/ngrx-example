{
  "name": "new-arch",
  "version": "1.0.0-SNAPSHOT",
  "license": "Commercial",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --baseHref=/ --sourceMap=true --proxy-config proxy.conf.json",
    "build": "ng build --prod --build-optimizer false",
    "test": "ng test --sourceMap=true --codeCoverage=true --browsers ChromeFlagsDisabled",
    "testci": "ng test --sourceMap=true --codeCoverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pretty-quick": "pretty-quick --staged",
    "sonar": "ng lint && ng test --sourceMap=true --codeCoverage=true --watch=false --browsers Chrome && sonar-scanner",
    "sonarci": "ng lint && sonar-scanner",
    "docs:generate": "compodoc -p src/tsconfig.app.json",
    "docs:server": "http-server documentation/ -o -p 1984",
    "docs": "npm run docs:generate && npm run docs:server"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "ng lint && npm run build"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/entity": "^8.3.0",
    "@ngrx/store": "^8.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "apollo": "^2.13.1",
    "apollo-angular": "^1.6.0",
    "apollo-angular-link-http": "^1.6.0",
    "apollo-cache-inmemory": "^1.3.2",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "core-js": "~2.5.4",
    "graphql-tag": "^2.10.0",
    "moment": "^2.24.0",
    "ngrx-store-localstorage": "^7.0.1",
    "ngx-currency": "^2.0.0",
    "ngx-mask": "8.1.6",
    "rxjs": "~6.3.3",
    "ts-node": "^8.2.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/pro-light-svg-icons": "^5.10.2",
    "@fortawesome/pro-regular-svg-icons": "^5.10.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-regular-svg-icons": "^5.10.2",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@ng-bootstrap/ng-bootstrap": "^4.2.1",
    "@ng-select/ng-select": "^2.18.0",
    "font-awesome": "^4.7.0",
    "sweetalert2": "8.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@compodoc/compodoc": "^1.1.10",
    "@ngrx/store-devtools": "^7.4.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "husky": "^2.4.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.18.2",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.2.2"
  }
}
